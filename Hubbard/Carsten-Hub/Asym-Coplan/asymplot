'4pt-OEP-Asym-mdotb.txt' u 1:4+2 w l lc rgb 'green' lw 2 title 'XX',\
'4pt-Slat-Asym-mdotb.txt' u 1:4+2 w l lc rgb 'blue' lw 2 title 'XXS', \
'4pt-STLS-Asym-mdotb.txt' u 1:4+2 w l lc rgb 'orange' lw 2 title 'STLS', \
'4pt-STC-Asym-mdotb.txt' u 1:4+2 w l lc rgb 'cyan' lw 2 title 'XXSc', \
'4pt-STLSc-Asym-mdotb.txt' u 1:4+2 w l lc rgb 'magenta' lw 2 title 'STLSc'

set output

set output '4pt-mdotb-site1-Asym.eps'

unset key

set ylabel 'm · B'
unset yrange
set ytics .5

p '4pt-B1-Asym-mdotb.txt' u 1:1+1 w l lc rgb 'red' lw 3 title 'Exact', \
'4pt-OEP-Asym-mdotb.txt' u 1:1+2 w l lc rgb 'green' lw 2 title 'XX',\
'4pt-Slat-Asym-mdotb.txt' u 1:1+2 w l lc rgb 'blue' lw 2 title 'XXS', \
'4pt-STLS-Asym-mdotb.txt' u 1:1+2 w l lc rgb 'orange' lw 2 title 'STLS', \
'4pt-STC-Asym-mdotb.txt' u 1:1+2 w l lc rgb 'cyan' lw 2 title 'XXSc', \
'4pt-STLSc-Asym-mdotb.txt' u 1:1+2 w l lc rgb 'magenta' lw 2 title 'STLSc'

set output

